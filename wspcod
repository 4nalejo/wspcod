for i in $(seq 1 60); do echo -en "_" ; done ; echo -e ""
echo -e "
clear();
console.log('▄▄▌ ▐ ▄▌.▄▄ ·  ▄▄▄· ▄▄·       ·▄▄▄▄');
console.log('██· █▌▐█▐█ ▀. ▐█ ▄█▐█ ▌▪▪     ██▪ ██');
